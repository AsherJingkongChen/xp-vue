{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "bundler",
    "types": [
      "cypress",
      "svelte",
      "vite/client",
      "vite-plugin-pwa/svelte"
    ]
  },
  "extends": "./.svelte-kit/tsconfig.json"
}
