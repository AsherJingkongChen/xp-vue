{
  "name": "xp-web",
  "version": "0.1.0",
  "author": {
    "email": "gpks556@gmail.com",
    "name": "AsherJingkongChen"
  },
  "bugs": "https://github.com/AsherJingkongChen/xp-web/issues",
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "description": "Web package of XP (Any file previewer)",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "gh-pages": "^6.1.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "~5.2.0",
    "vite": "^5.0.5",
    "vite-ssg": "^0.23.5",
    "vitest": "^1.0.1",
    "vue-tsc": "^1.8.25"
  },
  "homepage": "https://github.com/AsherJingkongChen/xp-web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsherJingkongChen/xp-web.git"
  },
  "scripts": {
    "build": "run-p type-check build-only build-only:gh-pages",
    "build-only": "vite-ssg build",
    "build-only:gh-pages": "vite-ssg build -c vite.config.gh-pages.ts",
    "deploy": "run-s prepublish && run-p deploy:gh-pages",
    "deploy:gh-pages": "cd dist-gh-pages && cp index.html 404.html && gh-pages -d .",
    "dev": "vite",
    "dev:test": "vitest",
    "format": "prettier --write .",
    "prepublish": "run-s format && run-p build test",
    "preview": "vite preview",
    "preview:gh-pages": "vite preview -c vite.config.gh-pages.ts",
    "test": "run-p test:unit",
    "test:unit": "vitest --run",
    "type-check": "vue-tsc --build --force"
  },
  "type": "module"
}
